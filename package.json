{
  "name": "@mytiki/receipt-capacitor",
  "version": "0.4.0",
  "description": "The TIKI Receipt library adds to your Vue.js + Capacitor mobile app a Data Reward program for your users to share their receipts in-exchange for loyalty points.",
  "author": "Tiki inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tiki/tiki-receipt-capacitor.git"
  },
  "bugs": {
    "url": "https://github.com/tiki/tiki-receipt-capacitor/issues"
  },
  "files": [
    "dist"
  ],
  "module": "./dist/receipt-capacitor.es.js",
  "main": "./dist/receipt-capacitor.umd.js",
  "typings": "./dist/types/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/main.d.ts",
      "import": "./dist/receipt-capacitor.es.js"
    },
    "./dist/receipt-capacitor.css": "./dist/receipt-capacitor.css"
  },
  "scripts": {
    "build": "rimraf dist && vue-tsc --declaration --emitDeclarationOnly; vite build",
    "example:android": "npm run build && cd example && npm run build && npx cap sync android && npx cap run android",
    "example:ios": "npm run build && cd example && npm run build && npx cap sync ios && npx cap run ios",
    "lint": "npm run eslint && npm run prettier -- --check",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write",
    "eslint": "eslint \"src/**/*.{ts,js}\"",
    "prettier": "prettier \"src/**/*.{css,html,ts,js,java}\""
  },
  "keywords": [
    "tiki",
    "microblink",
    "zero-party",
    "receipt"
  ],
  "dependencies": {
    "@capacitor/preferences": "^5.0.6",
    "@mytiki/capture-receipt-capacitor": "^0.6.2",
    "@mytiki/tiki-sdk-capacitor": "^0.3.1",
    "vue": "^3.3.4",
    "vue-markdown-render": "^2.0.1",
    "vue3-touch-events": "^4.1.3"
  },
  "devDependencies": {
    "@babel/types": "^7.22.5",
    "@capacitor/cli": "^5.4.1",
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/markdown-it": "^13.0.0",
    "@types/node": "^20.5.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.47.0",
    "eslint-plugin-vue": "^9.16.0",
    "path": "^0.12.7",
    "rollup-plugin-typescript2": "^0.36.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.2",
    "vite-plugin-vue-markdown": "^0.23.8",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.8.8"
  },
  "peerDependencies": {
    "@capacitor/android": "^5.4.1",
    "@capacitor/core": "^5.4.1",
    "@capacitor/ios": "^5.4.1",
    "@capacitor/preferences": "^5.0.6",
    "@mytiki/capture-receipt-capacitor": "^0.6.2",
    "@mytiki/tiki-sdk-capacitor": "^0.3.1",
    "vue": "^3.3.4",
    "vue3-touch-events": "^4.1.3"
  }
}
